<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>QuantConnect.Lean.DataSource.CascadeThetaData</AssemblyName>
    <RootNamespace>QuantConnect.Lean.DataSource.CascadeThetaData</RootNamespace>
    <!-- Disable warnings for using types from pre-built LEAN assemblies -->
    <NoWarn>$(NoWarn);CS1701;CS1702</NoWarn>
  </PropertyGroup>

  <!-- Use NuGet packages for LEAN dependencies (faster builds, no source conflicts) -->
  <ItemGroup>
    <PackageReference Include="QuantConnect.Common" Version="2.5.*" />
    <PackageReference Include="QuantConnect.Lean.Engine" Version="2.5.*" />
  </ItemGroup>

  <!-- Fallback to project references when building full solution (CI/local dev) -->
  <ItemGroup Condition="'$(UseLeanSource)' == 'true'">
    <ProjectReference Include="../../Common/QuantConnect.csproj" />
    <ProjectReference Include="../../Engine/QuantConnect.Lean.Engine.csproj" />
  </ItemGroup>
</Project>
